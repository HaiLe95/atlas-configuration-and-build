 {
  "roles": {
    "CREATE_AND_CONFIGURE": {
      "entityPermissions": [
        {
          "privileges": [
            "entity-read", "entity-create",
            "entity-read-classification",
            "entity-add-classification",
            "entity-update-classification"],
          "entityTypes":            [".*"],
          "entityIds":              [".*"],
          "entityClassifications":  [".*"],
          "labels":                 [".*"],
          "businessMetadata":       [".*"],
          "attributes":             [".*"],
          "classifications":        [".*"]
        }
      ],
      "relationshipPermissions": [
        {
          "privileges": [ "add-relationship", 
                          "update-relationship", 
                          "remove-relationship" ],
          "relationshipTypes":        [ ".*" ],
          "end1EntityType":           [ ".*" ],
          "end1EntityId":             [ ".*" ],
          "end1EntityClassification": [ ".*" ],
          "end2EntityType":           [ ".*" ],
          "end2EntityId":             [ ".*" ],
          "end2EntityClassification": [ ".*" ]
        }
      ]
    },

    "READ_ONLY": {
      "entityPermissions": [
        {
          "privileges": [ "entity-read", 
                          "entity-read-classification"],
          "entityTypes":            [".*"],
          "entityIds":              [".*"],
          "entityClassifications":  [".*"],
          "labels":                 [".*"],
          "businessMetadata":       [".*"],
          "attributes":             [".*"],
          "classifications":        [".*"]
        }
      ]
    }
  },


  "userRoles": {
    "read_only":      [ "READ_ONLY" ],
    "create_config":  [ "CREATE_AND_CONFIGURE" ]
  }
}

